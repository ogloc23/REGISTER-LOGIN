  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "start:prod": "node dist/index.js",
    "build": "npm run prisma:migrate:deploy && npm run prisma:generate && tsc",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "lint": "eslint . && echo 'Linting completed successfully: No issues found!'",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write . && echo 'Formatting completed successfully!'",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "apollo-server-express": "^3.13.0",
    "bcryptjs": "^3.0.2",
    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "graphql-tools": "^9.0.18",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.5.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
